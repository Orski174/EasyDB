-- Find pairs of products frequently ordered together (at least 5 times).

SELECT 
    o1.product_id AS product1, 
    o2.product_id AS product2, 
    COUNT(*) AS frequency
FROM 
    Orders o1
JOIN 
    Orders o2 ON o1.order_id = o2.order_id AND o1.product_id < o2.product_id
GROUP BY 
    o1.product_id, o2.product_id
HAVING 
    COUNT(*) >= 5
ORDER BY 
    frequency DESC;