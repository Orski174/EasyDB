-- List the names of employees who maintains more than one type of machine.

SELECT 
    e.SSN,
    e.F_Name, 
    e.L_Name,
    COUNT(DISTINCT CONCAT(mm.machine_number, '-', mm.machine_name)) AS machine_count
FROM 
    Employee e
JOIN 
    Maintains_Mach mm ON e.SSN = mm.EmpSSN
GROUP BY 
    e.SSN, e.F_Name, e.L_Name
HAVING 
    COUNT(DISTINCT CONCAT(mm.machine_number, '-', mm.machine_name)) > 3;
